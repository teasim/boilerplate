{
  "name": "teasim-boilerplate",
  "npmName": "TeasimBoilerplate",
  "version": "0.0.1-alpha",
  "description": "teasim boilerplate",
  "author": "Billgo",
  "license": "MIT",
  "scripts": {
    "preinstall": "node ./internals/scripts/npmcheckversion.js",
    "postinstall": "npm run build:dll",
    "presetup": "npm i chalk shelljs",
    "setup": "node ./internals/scripts/setup.js",
    "postsetup": "npm run build:dll",
    "clean": "shjs ./internals/scripts/clean.js",
    "prebuild": "rimraf ./build",
    "build": "cross-env NODE_ENV=production webpack --config internals/webpack/webpack.prod.babel.js --color -p --progress --hide-modules",
    "build:dll": "node ./internals/scripts/dependencies.js",
    "build:sample": "build-sample",
    "start": "cross-env NODE_ENV=development babel-node server",
    "start:production": "npm run test && npm run build && cross-env NODE_ENV=production babel-node server",
    "start:mocker": "mockers start",
    "sample": "start-sample -p 6060",
    "lint": "npm run lint:js && npm run lint:style",
    "lint:js": "prettier-eslint --write app/**/*.{js,jsx}",
    "lint:style": "stylelint-config-prettier-check app/**/*.{css,less}",
    "test": "NODE_ENV=test jest"
  },
  "jest": {
    "verbose": true,
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "app"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/mocks/fileMock.js",
      "\\.(css|less|scss)$": "<rootDir>/mocks/styleMock.js"
    },
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/build/",
      "/lib/",
      "/umd/",
      "/internals/"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.jsx?$",
    "testEnvironment": "enzyme",
    "setupTestFrameworkScriptFile": "./node_modules/jest-enzyme/lib/index.js",
    "unmockedModulePathPatterns": [
      "react",
      "enzyme",
      "jest-enzyme"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest"
    }
  },
  "eslintConfig": {
    "rules": {
      "prettier/prettier": "error"
    },
    "plugins": [
      "babel",
      "prettier"
    ],
    "extends": [
      "plugin:prettier/recommended",
      "prettier/flowtype",
      "prettier/react",
      "prettier/standard"
    ],
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "env": {
      "browser": true,
      "node": true,
      "jest": true,
      "es6": true
    }
  },
  "stylelint": {
    "extends": "stylelint-config-prettier",
    "rules": {
      "at-rule-no-unknown": [
        true,
        {
          "ignoreAtRules": [
            "extends"
          ]
        }
      ],
      "block-no-empty": null,
      "unit-whitelist": [
        "em",
        "rem",
        "s"
      ]
    }
  },
  "dllPlugin": {
    "path": "node_modules/teasim-boilerplate-dlls",
    "exclude": [
      "chalk",
      "compression",
      "cross-env",
      "express",
      "ip",
      "minimist"
    ],
    "include": [
      "core-js",
      "eventsource-polyfill"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/node": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@babel/runtime": "^7.1.2",
    "add-asset-html-webpack-plugin": "^3.0.1",
    "antd": "^3.10.0",
    "autoprefixer": "^9.1.5",
    "babel-core": "^7.0.0-bridge",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-import": "^1.9.1",
    "babel-preset-habitual": "^0.0.7",
    "chalk": "^2.4.1",
    "circular-dependency-plugin": "^5.0.2",
    "commander": "^2.18.0",
    "cpx": "^1.5.0",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "cssnano": "^4.1.4",
    "empty-module": "0.0.2",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "eslint": "^5.6.1",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-prettier": "^3.0.0",
    "exports-loader": "^0.7.0",
    "express": "^4.16.3",
    "file-loader": "^2.0.0",
    "fs-extra": "^7.0.0",
    "glob": "^7.1.3",
    "graceful-fs": "^4.1.11",
    "html-loader": "^0.5.5",
    "html-webpack-externals-plugin": "^3.8.0",
    "html-webpack-plugin": "^3.0.7",
    "http-proxy-middleware": "^0.19.0",
    "imports-loader": "^0.8.0",
    "ip": "^1.1.5",
    "jest": "^23.6.0",
    "jest-cli": "^23.6.0",
    "jest-environment-enzyme": "^6.1.2",
    "jest-enzyme": "^6.1.2",
    "jsx-loader": "^0.13.2",
    "less": "3.8.1",
    "less-loader": "^4.0.6",
    "mini-css-extract-plugin": "^0.4.3",
    "minimist": "^1.2.0",
    "node-plop": "^0.16.0",
    "offline-plugin": "^5.0.5",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss": "^7.0.5",
    "postcss-cli": "^6.0.0",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-less": "^3.0.2",
    "postcss-loader": "^3.0.0",
    "postcss-safe-parser": "^4.0.1",
    "prettier": "1.14.3",
    "prettier-eslint": "^8.8.2",
    "prettier-eslint-cli": "^4.7.1",
    "prettier-stylelint": "^0.4.2",
    "react-hot-loader": "^4.3.11",
    "redbox-react": "^1.6.0",
    "redux-devtools-extension": "^2.13.5",
    "regenerator-runtime": "^0.12.1",
    "rimraf": "^2.6.2",
    "shelljs": "^0.8.2",
    "style-loader": "^0.23.0",
    "stylelint": "^9.6.0",
    "stylelint-config-prettier": "^4.0.0",
    "teasim-fackers": "^5.0.4-22",
    "teasim-mockers": "^5.0.4-22",
    "teasim-samples": "^5.0.4-22",
    "teasim-scripts": "^5.0.4-22",
    "teasim-widget-sampleroom": "^5.0.4-22",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "url-loader": "^1.1.1",
    "webpack": "^4.20.2",
    "webpack-api-mocker": "^1.5.15",
    "webpack-bundle-analyzer": "^3.0.2",
    "webpack-cli": "^3.1.1",
    "webpack-dev-middleware": "^3.4.0",
    "webpack-hot-middleware": "^2.24.2"
  },
  "dependencies": {
    "core-js": "^2.5.6",
    "eventsource-polyfill": "^0.9.6",
    "immutable": "^3.8.2",
    "intl": "^1.2.5",
    "isomorphic-fetch": "^2.2.1",
    "moment": "^2.21.0",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-intl": "^2.6.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "5.0.0-alpha.9",
    "redux": "^4.0.0",
    "redux-immutable": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-saga": "^0.16.0",
    "reselect": "^4.0.0",
    "teasim": "^5.0.4-22",
    "teasim-plugin-loading": "^5.0.4-22"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/teasim/boilerplate.git"
  }
}

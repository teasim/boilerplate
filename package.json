{
  "name": "teasim-boilerplate",
  "npmName": "TeasimBoilerplate",
  "version": "0.0.1-alpha",
  "description": "teasim boilerplate",
  "author": "Billgo",
  "license": "MIT",
  "scripts": {
    "preinstall": "node ./internals/scripts/npmcheckversion.js",
    "postinstall": "npm run build:dll",
    "presetup": "npm i chalk shelljs",
    "setup": "node ./internals/scripts/setup.js",
    "postsetup": "npm run build:dll",
    "clean": "shjs ./internals/scripts/clean.js",
    "prebuild": "rimraf ./build",
    "build": "cross-env NODE_ENV=production webpack --config internals/webpack/webpack.prod.babel.js --color -p --progress --hide-modules",
    "build:dll": "node ./internals/scripts/dependencies.js",
    "build:sample": "build-sample",
    "start": "cross-env NODE_ENV=development babel-node server",
    "start:production": "npm run test && npm run build && cross-env NODE_ENV=production babel-node server",
    "start:mocker": "mockers start",
    "sample": "start-sample -p 6060",
    "pretest": "npm run test:clean",
    "test:clean": "rimraf ./coverage",
    "test": "cross-env NODE_ENV=test jest --coverage",
    "test:watch": "cross-env NODE_ENV=test jest --watchAll",
    "jest": "cross-env NODE_ENV=test jest"
  },
  "jest": {
    "collectCoverageFrom": [
      "app/**/*.{js,jsx}",
      "!app/**/*.test.{js,jsx}",
      "!app/*/RbGenerated*/*.{js,jsx}",
      "!app/index.js"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 98,
        "branches": 91,
        "functions": 98,
        "lines": 98
      }
    },
    "moduleDirectories": [
      "node_modules",
      "modules",
      "interalias",
      "packages",
      "app"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/internals/"
    ],
    "moduleNameMapper": {
      ".*\\.(css|less|styl|scss|sass)$": "<rootDir>/internals/mocks/cssModule.js",
      ".*\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/internals/mocks/image.js"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/internals/testing/test-bundler.js",
    "setupFiles": [
      "<rootDir>/internals/mocks/scrollbarSize.js"
    ],
    "testRegex": ".*\\.jest\\.js$",
    "verbose": true
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "plugin:jest/recommended",
      "plugin:react/recommended",
      "plugin:flowtype/recommended",
      "prettier",
      "prettier/flowtype",
      "prettier/react",
      "prettier/standard"
    ],
    "env": {
      "browser": true,
      "node": true,
      "jest": true,
      "es6": true
    },
    "plugins": [
      "babel",
      "react",
      "flowtype",
      "import",
      "jest",
      "prettier",
      "standard"
    ],
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "rules": {
      "prettier/prettier": "error"
    },
    "settings": {
      "import/resolver": {
        "webpack": {
          "config": "./internals/webpack/webpack.prod.babel.js"
        }
      },
      "flowtype": {
        "onlyFilesWithFlowAnnotation": true
      }
    }
  },
  "dllPlugin": {
    "path": "node_modules/teasim-boilerplate-dlls",
    "exclude": [
      "chalk",
      "compression",
      "cross-env",
      "express",
      "ip",
      "minimist"
    ],
    "include": [
      "core-js",
      "eventsource-polyfill"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/node": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@babel/runtime": "^7.1.2",
    "add-asset-html-webpack-plugin": "^3.0.1",
    "autoprefixer": "^9.1.5",
    "babel-core": "^7.0.0-bridge",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "babel-preset-habitual": "^0.0.7",
    "chalk": "^2.4.1",
    "child_process": "^1.0.2",
    "circular-dependency-plugin": "^5.0.2",
    "commander": "^2.18.0",
    "compression": "^1.7.3",
    "coveralls": "^3.0.2",
    "cpx": "^1.5.0",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "cssnano": "^4.1.1",
    "empty-module": "0.0.2",
    "eslint": "^5.6.1",
    "eslint-config-prettier": "^3.1.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-babel": "^5.2.0",
    "eslint-plugin-flowtype": "^2.50.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^21.24.1",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-standard": "^4.0.0",
    "expect": "^23.6.0",
    "exports-loader": "^0.7.0",
    "express": "^4.16.3",
    "file-loader": "^2.0.0",
    "flow-bin": "^0.82.0",
    "fs-extra": "^7.0.0",
    "glob": "^7.1.3",
    "graceful-fs": "^4.1.11",
    "html-loader": "^0.5.5",
    "html-webpack-externals-plugin": "^3.8.0",
    "html-webpack-plugin": "^3.0.7",
    "http-proxy-middleware": "^0.19.0",
    "imports-loader": "^0.8.0",
    "ip": "^1.1.5",
    "jest": "^23.6.0",
    "jest-cli": "^23.6.0",
    "jsx-loader": "^0.13.2",
    "less": "3.8.1",
    "less-loader": "^4.0.6",
    "less-vars-loader": "^1.1.0",
    "mini-css-extract-plugin": "^0.4.3",
    "minimist": "^1.2.0",
    "node-plop": "^0.16.0",
    "offline-plugin": "^5.0.5",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "plop": "^2.1.0",
    "postcss": "^7.0.5",
    "postcss-cli": "^6.0.0",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-loader": "^3.0.0",
    "postcss-safe-parser": "^4.0.1",
    "prettier": "^1.14.3",
    "react-hot-loader": "^4.3.11",
    "redbox-react": "^1.6.0",
    "redux-devtools-extension": "^2.13.5",
    "regenerator-runtime": "^0.12.1",
    "rimraf": "^2.6.2",
    "style-loader": "^0.23.0",
    "stylelint-webpack-plugin": "^0.10.5",
    "teasim-fackers": "^5.0.4-21",
    "teasim-mockers": "^5.0.4-21",
    "teasim-samples": "^5.0.4-21",
    "teasim-scripts": "^5.0.4-21",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "url-loader": "^1.1.1",
    "webpack": "^4.20.0",
    "webpack-api-mocker": "^1.5.15",
    "webpack-cli": "^3.1.2",
    "webpack-dev-middleware": "^3.4.0",
    "webpack-hot-middleware": "^2.24.2"
  },
  "dependencies": {
    "core-js": "^2.5.6",
    "eventsource-polyfill": "^0.9.6",
    "immutable": "^3.8.2",
    "intl": "^1.2.5",
    "isomorphic-fetch": "^2.2.1",
    "moment": "^2.21.0",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-intl": "^2.6.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "5.0.0-alpha.9",
    "redux": "^4.0.0",
    "redux-immutable": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-saga": "^0.16.0",
    "reselect": "^4.0.0",
    "teasim": "^5.0.4-21",
    "teasim-plugin-loading": "^5.0.4-21"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/teasim/boilerplate.git"
  }
}
